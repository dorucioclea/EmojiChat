{
  "name": "@wessberg/emojichat",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "compile": "tsc -p tsconfig.tool.json",
    "clean:dist": "rm -r -f dist",
    "sw:precache": "sw-precache --config=swprecache.config.json",
    "sw:fix": "node service-worker-replacement.js",
    "clean:compiled": "rm -r -f Tool/Compiled",
    "clean": "npm run clean:dist & npm run clean:compiled",
    "prepare": "npm run clean && npm run compile",
    "rollup": "rollup -c Tool/Compiled/Tool/Rollup/RollupConfig/rollup.config.js",
    "rollup:watch": "rollup -c Tool/Compiled/Tool/Rollup/RollupConfig/rollup.config.js --watch",
    "build:desktop": "npm run prepare && npm run rollup && npm run sw:precache && npm run sw:fix",
    "build:mobile": "npm run prepare && npm run rollup --mobile && npm run sw:precache && npm run sw:fix",
    "build:all": "npm run prepare && npm run rollup & npm run rollup --mobile && npm run sw:precache && npm run sw:fix",
    "build": "npm run build:all",
    "b": "npm run build",
    "b:desktop": "npm run build:desktop",
    "b:mobile": "npm run build:mobile",
    "b:all": "npm run build:all",
    "build:production": "NODE_ENV=production npm run build",
    "build:desktop:production": "NODE_ENV=production npm run build:desktop",
    "build:mobile:production": "NODE_ENV=production npm run build:mobile",
    "build:all:production": "NODE_ENV=production npm run build:all",
    "b:production": "npm run build:production",
    "b:desktop:production": "npm run build:desktop:production",
    "b:mobile:production": "npm run build:mobile:production",
    "b:all:production": "npm run build:all:production",
    "watch:desktop": "npm run prepare && npm run rollup:watch",
    "watch:mobile": "npm run prepare && npm run rollup:watch --mobile",
    "watch:all": "npm run prepare && npm run rollup:watch & npm run rollup:watch --mobile",
    "watch": "npm run watch:all",
    "watch:production": "NODE_ENV=production npm run watch",
    "watch:desktop:production": "npm run watch:production",
    "watch:mobile:production": "NODE_ENV=production npm run watch:mobile",
    "watch:all:production": "npm run watch:desktop:production & npm run watch:mobile:production",
    "w": "npm run watch",
    "w:desktop": "npm run watch:desktop",
    "w:mobile": "npm run watch:mobile",
    "w:all": "npm run watch:all",
    "w:production": "npm run watch:production",
    "w:desktop:production": "npm run watch:desktop:production",
    "w:mobile:production": "npm run watch:mobile:production",
    "w:all:production": "npm run watch:all:production",
    "serve": "run serve:all",
    "serve:desktop": "npm run watch:desktop --serve",
    "serve:mobile": "npm run watch:mobile --serve",
    "serve:all": "npm run watch:all --serve",
    "s": "npm run serve:all",
    "s:desktop": "npm run serve:desktop",
    "s:mobile": "npm run serve:mobile",
    "s:all": "npm run serve:all",
    "serve:production": "run serve:all:production",
    "serve:desktop:production": "npm run watch:desktop:production --serve",
    "serve:mobile:production": "npm run watch:mobile:production --serve",
    "serve:all:production": "NODE_ENV=production npm run serve:all",
    "s:production": "npm run serve:all:production",
    "s:desktop:production": "npm run serve:desktop:production",
    "s:mobile:production": "npm run serve:mobile:production",
    "s:all:production": "npm run serve:all:production",
    "deploy": "npm run build:production && ssh -i ~/.ssh/id.rsa frra@ssh.itu.dk 'cd public_html && rm -r -f EmojiChat && mkdir EmojiChat' && cd dist && scp -i ~/.ssh/id.rsa -r . frra@ssh.itu.dk:public_html/EmojiChat/"
  },
  "author": "Frederik Wessberg",
  "license": "MIT",
  "devDependencies": {
    "@types/mime": "1.3.1",
    "@types/node": "^8.0.9",
    "@wessberg/codeanalyzer": "^1.0.39",
    "@wessberg/fileloader": "^1.0.3",
    "@wessberg/filesaver": "^1.0.1",
    "@wessberg/iteration": "^1.0.2",
    "@wessberg/rollup-plugin-environment": "^1.0.1",
    "@wessberg/ts-config": "0.0.3",
    "cssnano": "^3.10.0",
    "html-minifier": "^3.5.2",
    "magic-string": "^0.22.2",
    "mime": "^1.3.6",
    "postcss": "^6.0.6",
    "postcss-cssnext": "^3.0.0",
    "postcss-hover": "^0.3.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-gzip": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript2": "^0.4.6",
    "rollup-watch": "^4.0.0",
    "sw-precache": "^5.2.0",
    "tslib": "^1.7.1",
    "typescript": "^2.4.1"
  },
  "dependencies": {
    "@wessberg/environment": "^1.0.1",
    "@wessberg/globalobject": "^1.0.5",
    "@wessberg/syncdb": "0.0.6"
  }
}
